<!DOCTYPE html>
<html lang="US">
    <head>
        <title>whatsdood</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta charset="utf-8">
        <meta name="description" content="<%- siteDescription %>">
        <meta property="og:type" content="Articles" />
        <meta property="og:description" content="<%- siteDescription %>"/>
        <meta property="og:url" content="<%- siteURL %>" />
        <meta property="og:site_name" content="<%- siteName %>" />
        <link href="../assets/index.css" rel="stylesheet" type="text/css">
        <link rel="icon" type="image/png" href="./assets/1391017.png"/>
        <link href="https://fonts.googleapis.com/css?family=ABeeZee" rel="stylesheet">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> 
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    </head>
    <body>
      <header>
        <!--sidebar nav-bar-->
        <div id="myNav" class="overlay">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <div class="overlay-content">
              <a href="/">#</a>
              <a href="/signin">Login</a>
            </div> 
        </div>

      <!--<div class="toolbar">
         <div class="open_icon" onclick="openNav()">&#9776;</div>
         <span class="blog_icon">whatsdood</span>
       </div>--> 
      <div class="toolbar">
        <div class="open_icon" onclick="openNav()">&#9776;</div>
        <span class="blog_icon">
         <img src="../assets/icon.svg" alt="<%- siteName %>" />
        </span>
      </div>
</header>
        

      <section>
        <% for(let i=0;i < data.length;i++) { %>
          <div>
           <% if(data[i]._imageFromSearch == 'noImageFound') { %>
            <div class="article_title onlytext">
              <a href="/article/<%= data[i].title.split(' ').join('-') %>" target="_self"><%= data[i].title%> </a>
            </div>
            <% } else { %>
            <div class="article_image">
              <img class="image" src="<%= data[i]._imageFromSearch %>"  alt=""/>
            </div>
                  
            <div class="article_title">
              <a href="/article/<%= data[i].title.split(' ').join('-') %>" target="_self"><%= data[i].title%> </a>
            </div>
              <% } %>
          </div>
         
       <% } %>
      </section>

      <div class="bottom-toolbar">
        <% if (pages > 0) { %>
          <div class="paginationtext-center">
            <% if(current == 1) { %>
              <span class="disabled">
                <a>First</a>
              </span>
            <%} else { %>
              <span>
                <a href="/page/1">First</a>
              </span>
            <%}%>
            <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
            <% if(i !== 1){ %>
              <span class="disabled">
                <a>...</a>
              </span>
            <%}%>
            <% for(; i <= (Number(current) + 4) && i <= pages;i++) { %>
              <% if(i == current) { %>
                <span class="active">
                  <a><%= i %></a>
                </span>
              <%} else {%>
                <span>
                  <a href="/page/<%= i %>"><%= i %></a>
                </span>
                <%}%>
                <% if(i == Number(current) + 4  && i < pages ) { %>
                  <span class="disabled">
                    <a>...</a>
                  </span>
                <%}%>
            <%}%>
            <% if(current == pages) { %> 
              <span class="disabled">
                <a>Last</a>
              </span>
            <%} else {%>
              <span>
                <a href="/page/<%= pages %>">Last</a>
            </span>
              <%}%>
            </div>
        <%}%>
      </div>
    </body>
    <script src="../assets/sidebarhelper.js"></script>
</html>