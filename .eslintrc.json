{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "quotes":[
            "error",
            "double"
        ],
        "linebreak-style": ["error","windows"],
        "newline-after-var":"off",
        "no-unused-expressions":["error",{"allowTernary": true}]
    }
}
